#!groovy

@Library("Infrastructure")

String product = "div"
String component = "hm"

withPipeline("nodejs", product, component) {
  if (env.CHANGE_TITLE && !env.CHANGE_TITLE.startsWith('[PREVIEW]')) {
    setVaultName('div')
    enableDockerBuild()
    enableDeployToAKS()
  }

  enableSlackNotifications('#div-dev')
}
